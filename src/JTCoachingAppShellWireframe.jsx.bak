function ClientSessionNotesTab({ client, onOpenSession }) {
  const sessionStatuses = ["Draft ready", "Sent", "Sent", "Draft generating", "Waiting for transcript"];

  return (
    <div className="space-y-6">
      <div className="flex items-center justify-between">
        <h3 className="text-lg font-semibold">Session Notes ({client.notes})</h3>
        <button className="px-4 py-2 bg-gray-900 text-white rounded hover:bg-gray-800">
          + New Note
        </button>
      </div>

      <div className="space-y-3">
        {Array.from({length: Math.min(5, client.notes)}).map((_, idx) => {
          const status = sessionStatuses[idx % sessionStatuses.length];
          const date = new Date(client.lastSession);
          date.setDate(date.getDate() - (idx * 7));
          const sessionId = `${client.id}-${idx}`;

          return (
            <button
              key={idx}
              onClick={() => status === "Draft ready" && onOpenSession && onOpenSession(sessionId)}
              className="w-full bg-white p-4 rounded-lg shadow hover:shadow-md transition cursor-pointer text-left"
            >
              <div className="flex items-center justify-between">
                <div className="flex-1">
                  <div className="font-medium">Session {client.notes - idx}</div>
                  <div className="text-sm text-gray-600">{date.toLocaleDateString()}</div>
                  {status === "Draft ready" && (
                    <div className="text-xs text-blue-600 mt-1">‚Üí Click to review and edit</div>
                  )}
                </div>
                <span className={[
                  "text-xs px-3 py-1 rounded-full font-medium",
                  status === "Draft ready" ? "bg-yellow-100 text-yellow-800" :
                  status === "Sent" ? "bg-green-100 text-green-800" :
                  status === "Draft generating" ? "bg-blue-100 text-blue-800" :
                  "bg-gray-100 text-gray-600"
                ].join(" ")}>
                  {status}
                </span>
              </div>
            </button>
          );
        })}
      </div>
    </div>
  );
}

